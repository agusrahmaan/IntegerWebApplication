@model Car[]

@{
    ViewData["Title"] = "Car Page";
}

<div>
    <h1>@ViewData["Title"]</h1>
    <table class="table-bordered table table-striped">
        <thead>
            <tr>
                <th>ID Registration</th>
                <th>Type</th>
                <th>Varian</th>
                <th>Brand</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var item in ViewBag.Cars)
            {
                <tr>
                    <td>@item.IDRegistration</td>
                    <td>@item.Type</td>
                    <td>@item.Brand</td>
                    <td>@item.Varian</td>
                </tr>
            }
        </tbody>
    </table>
</div>
